PUT /indy
PUT /audit

PUT /indy/_mapping/tracking_summary
{
  "properties": {
    "trackingID": {
      "type": "text"
    },
    "startTime": {
      "type": "date"
    },
    "endTime": {
      "type": "date"
    }
  }
}

GET /indy/_mapping/tracking_summary

# Create the type
PUT /audit/_mapping/content_events
{
  "properties": {
    "eventId": {
      "type": "text"
    },
    "sessionId": {
      "type": "text",
      "fielddata": true
    },
    "nodeId": {
      "type": "text"
    },
    "checksum": {
      "type": "text"
    },
    "targetLocation": {
      "type": "text"
    },
    "targetPath": {
      "type": "text"
    },
    "eventType": {
      "type": "text"
    },
    "requestId": {
      "type": "text"
    },
    "eventVersion": {
      "type": "integer"
    },
    "extra": {
      "properties": {
        "trackingId": {
          "type": "text",
          "fielddata": true
        }
      }
    }
  }
}

